<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.metedology">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_fish"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"

        android:testOnly="false"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityWaterfall" />
        <activity android:name=".ActivityRup" />
        <activity android:name=".ActivityAgile"></activity>

       <!-- <activity android:name=".view.GameBeginDialog"></activity> -->

    </application>

</manifest>